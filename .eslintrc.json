{
  "extends": ["airbnb-base", "prettier", "plugin:unicorn/recommended"],
  "plugins": ["unicorn", "prettier", "typescript"],
  "parser": "typescript-eslint-parser",
  "rules": {
    "prettier/prettier": ["error"],
    "no-console": 0,
    "no-plusplus": ["error", { "allowForLoopAfterthoughts": true }]
  }
}
